{% extends 'layout.njk' %}

{% block title %}YESM 2020 | Sponsors{% endblock %}
{% block desc %}Sponsors of YESM. Young Economist Students' Meet 2019 (YESM 2019). Venue: Jadavpur University. Department of Economics. Designer: Vedant Mozumdar.{% endblock %}

{% block metahead %}
    <link rel="stylesheet" href="css/sponsor.css">
    <script src="js/sponsor.js"></script>
{% endblock %}

{% block content %}
<body id="sponsor">
    {% import 'macros/header.njk' as nav %}
    {{ nav.gen('s') }}
    <main id="main">
        <section>
            <h1>Sponsors</h1>
            <div>
                <div class="img-wrap" style="width:180px;height:300px;max-width:600px;max-height:300px;">
                    <img class="lazy" src="img/sponsors/lcssr.jpg" alt="Hatari">
                </div>
                <p>Title Sponsor</p>
            </div>
            <div>
                <div class="img-wrap" style="width:200px;height:250px;max-width:444px;max-height:250px;">
                    <img class="lazy" src="img/sponsors/WBPDCL%20Logo_Blue%20Colour.jpg" alt="Gafa">
                </div>
                <p>Associate Sponsor</p>
            </div>
            <div>
                <div class="img-wrap" style="width:300px;height:250px;">
                    <img class="lazy" data-src="catalyst.jpeg" src="data:image/jpeg;base64," alt="Catalyst Learning Services">
                </div>
                <p>Career Partner</p>
            </div>
            <div>
                <div class="img-wrap" style="width:345px;height:213px;">
                    <img class="lazy" data-src="edusure.png" src="data:image/jpeg;base64," alt="Edusure">
                </div>
                <p>Education Sponsor</p>
            </div>
            
            <div>
                <div class="img-wrap" style="width:299px;height:300px;">
                    <img class="lazy" src="img/sponsors/Londonloungelogo.jpg" alt="Grissini">
                </div>
                <p>Food Sponsor</p>
            </div>
           
        </section>
    </main>
</body>
{% endblock %}
